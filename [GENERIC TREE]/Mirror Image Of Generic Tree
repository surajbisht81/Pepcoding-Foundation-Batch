 public static void mirror(Node node){
    // write your code here
    
    if(node==null){
        return;
    }
    if(node.children.size()<2){
        return;
    }
    for(int i = 0; i<node.children.size(); i++){
        mirror(node.children.get(i));
    }
     Collections.reverse(node.children);
  }
