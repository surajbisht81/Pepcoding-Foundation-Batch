  public static void levelOrder(Node node) {
    // write your code here
    Queue<Node> queue =  new LinkedList<> ();
    queue.add(node);
    while(queue.size()!=0){
       int currNode = queue.size();
       if(currNode == 0){break;}
       while(currNode>0){
           Node nodeNew = queue.peek();
           System.out.print(nodeNew.data+" ");
           queue.remove();
           if(nodeNew.left!=null){
               queue.add(nodeNew.left);
           }
           if(nodeNew.right!=null){
               queue.add(nodeNew.right);
           }
           currNode--;
       }
       System.out.println();
    }
  }
